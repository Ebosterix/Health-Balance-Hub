# Frontend Environment Variables for Vite
# Copy this file to .env and fill in your actual values

# URL of your backend server (Express API)
VITE_BACKEND_URL=http://localhost:5001

# Node environment (for debugging)
VITE_NODE_ENV=development

# Application name
VITE_APP_NAME=HBH_Frontend

# API URL (for API calls)
VITE_API_URL=http://localhost:5001/api

# Production Configuration (Oracle Cloud)
# VITE_BACKEND_URL=http://YOUR_ORACLE_PUBLIC_IP:5001
# VITE_API_URL=http://YOUR_ORACLE_PUBLIC_IP:5001/api
# VITE_NODE_ENV=production
